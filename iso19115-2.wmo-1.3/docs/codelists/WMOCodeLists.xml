            <gml:identifier codeSpace="WMO Core Metadata Profile">WMO_DistributionScopeCode</gml:identifier>
            <gmx:codeEntry>
                <gmx:CodeDefinition gml:id="WMO_DistributionScopeCode_GlobalExchange">
                    <gml:description>Data are published for global exchange via the WIS. Data shall be incorporated into the GISC Cache.</gml:description>
